$(function(){resize()});
function resize() {
height = $(window).height() -50;
b_mar = height*marging;
t_mar = height - height*marging;
$(".scene").height( height );
$(".image-container").width( img_count * img_width / categs +20);
}

var categs = 3;
var img_width = 322;
var img_count = 0;

var timeoutHandle = false;
var height = 640;
var marging = .4;
var t_mar = height - height*marging;
var b_mar = height*marging;
var diff = 0;
var section = 0;
var sec_off = 0;
var pepe=true;
var prev=0;
var th=false;
$(window).scroll(function() {

       var handler =  function(e) { return true;
              diff = $(window).scrollTop() % height;
              section = ~~( $(window).scrollTop() / height );
              sec_off = - (section*height) + $(window).scrollTop();
              if(!sec_off) return;
              //alert($(window).scrollTop()+" "+height+" "+section+" "+sec_off);
              if(sec_off > t_mar) { pepe=false; $('body,html').stop().animate({scrollTop: (section*height+height)},function(){ pepe=true });  }
              if(sec_off < b_mar) { pepe=false; $('body,html').stop().animate({scrollTop: (section*height)},function(){ pepe=true });  }
              console.log("sec " + sec_off);
       }

       $(this).scroll(function(e) {
           if (timeoutHandle) clearTimeout(timeoutHandle);

           if (pepe) timeoutHandle = setTimeout(function() {
             handler(e);
           }, 200);
       });

})

$('a').click(function(e){
 e.preventDefault();
 $('body,html').stop().animate({scrollTop: ($(e.target).parent().attr("to")*(height+50))+"px"});
})

$('.toleft').click(function(e){
     $(".image-scroller").animate({scrollLeft:'-=966px'});
});
$('.toright').click(function(e){
     $(".image-scroller").animate({scrollLeft:'+=966px'});
});

$(function(){
  $.ajax({
    url:'/images.json', 
    success: function(resp){
      img_count = resp.length;
      $(".image-container").html( tmpl("tmpl-plato", resp) );
      resize();
    }
  });
})
          </script>
