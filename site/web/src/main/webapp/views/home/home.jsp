<div class="home dynamicBackground" data-speed="0.5" data-pos="0" style="background-image: url('{%= resto.profile.homeBkg %}'); ">
    <div class="markup"></div>
    <div class="container_12">
        <div class="contentBox grid_5">
            <div class="restBox">
                <div class="title">
                    {%= resto.profile.name %}
                </div>
                <div class="desc">
                    {%= resto.profile.description %}
                </div>
            </div>
        </div>
    </div>
    <div class="socialButtons clearfix">
        <div class="socialButton facebookButton"></div>
        <div class="socialButton twitterButton"></div>
        <div class="socialButton youtubeButton"></div>
    </div>
</div>
